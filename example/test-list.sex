(import list)

(struct foo
  ((float a-field)
   (int b)
   ((const char *) c)
   ((fn bool ((bool val))) not)))

(var (struct foo) f)

(list-T int)
(make-list-T int #f)
(add-value-list-T int #f)
(length-list-T int #f)
(is-empty-list-T int #f)

(extern fn void puk ((int a) (float b)))
